                        <input type="button" value="Delete" style="font-size: 12px" onclick="grid.deleteSelectedRows()" />
                        
                        <script type="text/javascript" charset="UTF-8">
                            var grid = new dhtmlXGridObject("grid_here");
                            grid.setImagePath("assets/imgs/");
                            grid.setHeader("Customer,Region,Country,ActixOne,Architecture,Analyzer,Google Map,Bing Map,Protone,Elka,Platform");
                            grid.attachHeader("#text_filter,#text_filter,#text_filter,#text_filter,#text_filter,#text_filter,#text_filter,#text_filter,#text_filter,#text_filter,#text_filter");
