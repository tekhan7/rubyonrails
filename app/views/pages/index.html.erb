                            //grid.setInitWidths("100,100,100,100,100,100,100,100,100,100,100");
                            grid.setInitWidths("");
                            grid.setSkin("dhx_web");
                            grid.init();

                            grid.load("<%= data_path %>", "json");
                            dp = new dataProcessor("<%= db_action_path %>");
                            dp.init(grid);
                        </script>
                    </div>
